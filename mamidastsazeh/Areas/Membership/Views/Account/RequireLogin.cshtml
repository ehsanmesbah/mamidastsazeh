@model string

@{
    ViewBag.Title = "دسترسی نیازمند ورود است";
    Layout = "_Layout";
}
<div class="container-fluid">
   
        <div class="d-flex flex-column align-items-center justify-content-center w-100">

            <div class="card col-10 col-md-6 col-lg-6 shadow mt-3">
                <div class="card-body">
                    <h1 class="text-center mb-2" style="color:#ea1d5d;">برای مشاهده آموزش های بخش کسب و کار باید عضو سایت باشید. عضویت رایگان است</h1>
                    
                    <div class="text-center w-100">
                        <a asp-action="Login" class="btn btn-link text-decoration-none text-center" asp-route-returnUrl="@Model" style="color:blue;">
                            ورود اعضا
                        </a>
                    </div>
                    <div class="text-center w-100">
                        <a asp-action="VerifyMobile" class="btn btn-link text-decoration-none text-center" asp-route-returnUrl="@Model" style="color:blue;">
                            ثبت نام
                        </a>
                    </div>
                    <form></form>

                </div>
            </div>
        </div>
</div>

    @section ViewScript  {
        <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
        <script src="~/lib/jquery-validation-unobtrusive/dist/jquery.validate.unobtrusive.min.js"></script>
    }
