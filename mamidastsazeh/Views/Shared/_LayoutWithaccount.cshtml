@inject IHashId HashId;

<!DOCTYPE html>

<html lang="fa">
<head>
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-178741593-1"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'UA-178741593-1');
    </script>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    @if (ViewData["title"] == null || ViewData["title"].ToString() == "")
    {

        <title>آموزش هنرهای خانگی مادر و کودک</title>
    }
    else
    {
        <title>@ViewData["title"]</title>
    }

    <link rel="stylesheet" href="~/lib/fonts/Yekan.min.css" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/lib/fontawesome/css/all.min.css" />

    <link rel="stylesheet" href="~/css/Layout.min.css" />
    <link rel="stylesheet" href="~/css/PostSlider.min.css" />
    <link rel="stylesheet" href="~/css/Auth.min.css" />


    <link rel="stylesheet" href="~/css/Post.min.css" />
    <link rel="shortcut icon" href="~/images/favicon.ico">
    <link rel="icon" type="image/gif" href="~/images/animated_favicon1.gif">
    <link rel="stylesheet" href="~/css/myswipper.min.css" />

    <link rel="stylesheet" href="~/lib/swiper/swiper-bundle.min.css" />
    <link rel="stylesheet" href="~/css/mymenu.min.css">
    <link rel="stylesheet" href="~/css/ProfileDropDown.min.css">
    <link rel="stylesheet" href="~/lib/m-custom-scrollbar/css/jquery.mCustomScrollbar.min.css" />
    @await RenderSectionAsync("ViewStyleSheet", required: false)
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/swiper/swiper-bundle.min.js"></script>
    <script src="~/lib/propperjs/dist/umd/popper.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/js/MySwiper.min.js"></script>
    <script src="~/js/mymenu.min.js"></script>
    <script src="~/lib/m-custom-scrollbar/js/jquery.mCustomScrollbar.min.js"></script>
    @await RenderSectionAsync("ViewScript", required: false)
    <meta name="description"
          content="مر جع آموزش هنری دست سازه ساز جهت بانوان مادران رنگ آمیزی دکوپاژ دانلود دکوری تزئینات کاردستی عید دانلود تم تولد" />
    <meta name="keywords"
          content="رنگ , آموزش دست سازه, آموزش کاردستی , دکوپاژ,  دانلود تم تولد,  ساخت , بازسازی ,  ایده های کسب و کار خانگی,  هنری,  ایرانی, mamidastsaz, مامی دستساز ,  mamidastsazeh, اینستاگرام ,  مادرانه ,  بانوان" />
</head>
<body>
    <header class="layout-header">

        <nav class="navbar navbar-expand-sm py-0 pt-3 pt-md-1 pb-0" style="background-color: #FFFFFF ; position:relative;">
            <div class="container d-flex justify-content-between px-1 px-md-2 px-lg-5 py-1">
                <div class="d-flex justify-content-start align-items-center">
                    <a href="https://mamidastsazeh.com" class="logo ml-2">
                        <img src="~/images/loggo-mamidastsazeh.jpg" alt="مامی دست سازه" class="rounded-pill" />
                    </a>


                </div>
                <div class="search-container d-none d-md-block position-relative">
                    <div class="search d-none d-md-flex">
                        <input type="text" class="form-control" placeholder="جستجوی آموزش ها  ..." />
                        <a href="#" class="search-submit pl-4">
                            <i class="fas fa-search"></i>
                        </a>
                    </div>
                    <div class="search-suggest" style="z-index:1100;"></div>
                    <div class="search-overlay" style="z-index:1099;"></div>
                </div>
                <div class="d-flex align-items-center">
                    <vc:profile-drop-down></vc:profile-drop-down>

                </div>
            </div>

        </nav>
        <div class="mobile-nav">
            
            <a href="#" class="col-6 toggle-btn" id="toggle" style="padding-top: 10px;align-items:center;">
                <img src="~/images/dastebandi.png" alt="دسته بندی" style="width:100%;" />
            </a>
            <a href="https://mamidastsazeh.com" class="col-6 toggle-help" style="padding-top: 10px; align-items: center;"><img src="~/images/menulogonew.png" alt="مامی دست سازه" style="width:100%;"/></a>
        </div>
        <nav class="navigation" style="z-index:1000;">
            <div class="container">
                <ul class="menu-level-1">
                    <!-- Menu Level 1 -->
                    <li class="menu-list-1">
                        <a href="#" class="list-item">

                            <span>پست های آموزشی</span>
                            <img class="item-icon" src="data:image/png;base64," alt="">
                        </a>
                        <vc:main-categories list-type="All" strid=0></vc:main-categories>
                    </li>
                    <li class="menu-list-1">
                        <a href="#" class="list-item">

                            <span>ویژه ها</span>
                            <img class="item-icon" src="data:image/png;base64," alt="">
                        </a>

                        <ul class="menu-level-2">
                            <!-- Menu Level 2 -->
                            <li class="menu-list-2">
                                <a asp-action="Index" asp-controller="SpecialPost" asp-route-pageType="new" asp-route-pageNumber=1 class="list-item">
                                    <img src="data:image/png;base64," alt="mobile">
                                    <span>جدیدترین</span>
                                </a>
                            </li>
                            <li class="menu-list-2">
                                <a asp-action="Index" asp-controller="SpecialPost" asp-route-pageType="mostview" asp-route-pageNumber=1 class="list-item">
                                    <img src="data:image/png;base64," alt="mobile">
                                    <span>پر بازدیدترین</span>
                                </a>
                            </li>
                            <li class="menu-list-2">
                                <a asp-action="Index" asp-controller="SpecialPost" asp-route-pageType="mostlike" asp-route-pageNumber=1 class="list-item">
                                    <img src="data:image/png;base64," alt="mobile">
                                    <span>بیشترین لایک</span>
                                </a>
                            </li>
                            <li class="menu-list-2">
                                <a asp-action="Index" asp-controller="SpecialPost" asp-route-pageType="mostcomment" class="list-item">
                                    <img src="data:image/png;base64," alt="mobile">
                                    <span>بیشترین کامنت</span>
                                </a>
                            </li>
                        </ul>
                    </li>
                    <li class="menu-list-1">
                        <a class="list-item" asp-action="Index" asp-controller="SearchPage">

                            <span>جستجوی پیشرفته</span>
                            <img class="item-icon" src="data:image/png;base64," alt="">
                        </a>
                    </li>

                    <li class="menu-list-1">
                        <a class="list-item" asp-action="Index" asp-controller="MainCategory" asp-route-hashId=@HashId.Encode(6) asp-route-categoryName="دانلود">
                            <span>دانلود</span>
                            <img class="item-icon" src="data:image/png;base64," alt="">
                        </a>
                    </li>
                    @if (!Context.User.Identity.IsAuthenticated)
                    {
                        <li class="menu-list-1">

                            <a class="list-item" asp-area="Membership" asp-controller="Account" asp-action="VerifyMobile">

                                <span>ثبت نام</span>
                                <img class="item-icon" src="data:image/png;base64," alt="">
                            </a>
                        </li>
                    }
                    @if (Context.User.Identity.IsAuthenticated)
                    {
                        <li class="menu-list-1">

                            <a class="list-item" asp-area="Membership" asp-controller="Panel" asp-action="UserSettings">

                                <span>ناحیه کاربری</span>
                                <img class="item-icon" src="data:image/png;base64," alt="">
                            </a>
                        </li>
                        <li class="menu-list-1">

                            <a class="list-item" asp-area="Membership" asp-controller="Account" asp-action="Logout">

                                <span>خروج</span>
                                <img class="item-icon" src="data:image/png;base64," alt="">
                            </a>
                        </li>
                    }


                </ul><!-- End Menu Level 1 -->
            </div>
        </nav>



    </header>
    <main>
        @RenderBody()
    </main>
    <footer>
        <div class="container-fluid bg-light">
            <div class="container">
                <div class="row py-5">
                    <div class="col-md-6 col-lg-4 mb-5 mb-md-0">
                        <div class="container">

                            <div class="row mb-2 mb-md-4">
                                <div class="col p-0">
                                    <p class="text-muted small">در مامی دست سازه شما قادر خواهید بود آموزش های دست سازه های خود را با دیگران به اشتراک بگذارید.</p>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col p-0">
                                    <a class="btn btn-pink btn-sm w-100">
                                        <span>
                                            <i class="fas fa-user-plus"></i>
                                            <span class="btn-pink-text">به مامی دست سازه بپیوندید</span>
                                        </span>
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 col-lg-4 mb-5 mb-md-0">
                        <div class="container">
                            <div class="row">
                                <div class="col p-0 extra-links">
                                    <h5>دیگر صفحات مامی دست سازه</h5>
                                    <ul class="list-unstyled">
                                        <li><a href="#">تبلیغات</a></li>
                                        <li><a href="#">قوانین</a></li>
                                        <li><a href="#">تماس با ما</a></li>
                                        <li><a href="#">به ما پیوندید</a></li>
                                        <li><a href="#">پرسش های متداول</a></li>

                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
        <div class="container-fluid footer-bottom">
            <div class="container px-0 py-5">
                <div class="row no-gutters">
                    <div class="col-12 col-sm-10 col-md-7 col-lg-5 col-xl-5 offset-xl-4 mx-auto mx-xl-0 ml-xl-auto">
                        <p class="text-muted small text-center text-xl-right copyright">مسئولیت محتوای ویدئوها و هرگونه پاسخگویی به ادعاهای مطروحه توسط اشخاص حقیقی و حقوقی با منتشر کننده است و مامی دست سازه هیچگونه مسئولیتی نسبت به آن ندارد.</p>
                    </div>
                    <div class="w-100 d-xl-none"></div>
                    <div class="col-9 col-sm-5 col-md-4 col-lg-3 col-xl-3 px-xl-4 mx-auto mx-xl-0 mr-xl-auto">
                        <p class="text-muted text-center small">مامی دست سازه را در شبکه های اجتماعی دنبال کنید</p>
                        <div class="d-flex mx-4 justify-content-between align-items-center">
                            <a href="#" class="social-media telegram">
                                <i class="fab fa-telegram-plane mx-auto"></i>
                            </a>

                            <a href="https://www.instagram.com/mamidastsazeh/" class="social-media instagram">
                                <i class="fab fa-instagram mx-auto"></i>
                            </a>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>
</body>
</html>
